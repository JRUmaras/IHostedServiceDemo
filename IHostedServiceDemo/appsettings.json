{
  "HostedServiceSettings": {
    "PingIntervalInSeconds": 2
  },
  "Serilog": {
    "Using":  [ "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { 
        "Name": "File", 
        "Args": { 
          "path": "Logs/log-.log", 
          "rollingInterval": "Day"
        }
      }
    ]
  }
}